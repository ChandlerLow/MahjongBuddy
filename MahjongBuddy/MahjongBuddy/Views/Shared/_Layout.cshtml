<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.4/angular.min.js"></script>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" media="screen"/>
    <link rel="stylesheet" href="~/Content/bootstrap-theme.min.css" media="screen" />
    <link rel="stylesheet" href="~/Content/bootstrap-toggle.min.css" media="screen" />
    <link rel="stylesheet" href="~/Content/animate.min.css" media="screen" />
    <script src="~/Scripts/bootstrap.min.js"></script>    
    <script src="~/Scripts/app.js"></script>
    <script src="~/Scripts/controller.js"></script>
    <script src="~/Scripts/bootstrap-toggle.min.js"></script>
    <script src="~/Scripts/match-media.js"></script>
    <script src="~/Scripts/toggle-checkbox.js"></script>
    <script src="~/Scripts/ngDraggable.js"></script>
    <script src="~/Scripts/angular-animate-1.4.4.js"></script>
    <script src="~/Scripts/mjService.js"></script>
    <script src="~/Scripts/signalrService.js"></script>

    @RenderSection("head", required: false)
</head>
<body>
    @RenderBody()

    
    @RenderSection("scripts", required: false)
</body>
</html>
